{
  "name": "@oimdb/snapshot-manager",
  "version": "1.0.0",
  "description": "Snapshot manager for OIMDB collections",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@oimdb/core": "^1.0.6"
  },
  "keywords": [
    "oimdb",
    "snapshot",
    "reactive",
    "collection",
    "database"
  ],
  "author": "OIMDB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oimdb/oimdb.git",
    "directory": "packages/snapshot-manager"
  }
}
