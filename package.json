{
  "name": "oimdb",
  "version": "1.0.0",
  "description": "Fast, ergonomic in‑memory data library with type‑safe indices, reactive subscriptions, single‑layer coalescing, delta‑only updates, and an instance‑based DB runtime.",
  "keywords": [
    "typescript",
    "library",
    "oim",
    "database",
    "oimdb",
    "state-management",
    "state-manager"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abaikov/oimdb.git"
  },
  "bugs": {
    "url": "https://github.com/abaikov/oimdb/issues"
  },
  "homepage": "https://github.com/abaikov/oimdb#readme",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dx": {
      "import": "./dist/dx/index.js",
      "require": "./dist/dx/index.cjs",
      "types": "./dist/dx/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs && tsup src/dx/index.ts --dts --format esm,cjs --out-dir dist/dx",
    "bench": "tsx scripts/run-bench.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:build": "node scripts/test-build.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "author": "Andrei Baikov",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.2.2"
  }
}
